{
    "name": "infyomlabs/laravel-ui-purecss",
    "description": "Laravel frontend preset for PureCSS Theme",
    "keywords": [
        "laravel",
        "preset",
        "purecss"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mitul Golakiya",
            "email": "me@mitul.me"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "infyomlabs/laravel-generator-helpers": "^4.0",
        "laravel/ui": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\PureCSSPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InfyOm\\PureCSSPreset\\PureCSSPresetServiceProvider"
            ]
        }
    },
    "funding": [
        {
            "type": "opencollective",
            "url": "https://opencollective.com/infyomlabs"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
